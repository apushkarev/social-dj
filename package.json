{
  "name": "statement-dj",
  "version": "0.1.0",
  "description": "Music library manager",
  "main": "electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron": "vite build && electron .",
    "electron:dev": "concurrently \"vite --port 5173\" \"wait-on http://localhost:5173 && electron .\""
  },
  "dependencies": {
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "concurrently": "^9.0.0",
    "electron": "^33.0.0",
    "vite": "^6.0.0",
    "wait-on": "^8.0.0"
  }
}
